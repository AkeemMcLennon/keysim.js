{
  "name": "keysim",
  "version": "1.1.2",
  "description": "Keyboard simulator for JavaScript.",
  "main": "dist/keysim.js",
  "jsnext:main": "lib/keysim.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eventualbuddha/keysim.js"
  },
  "scripts": {
    "build": "gobble build dist -f",
    "test": "mocha -R spec --recursive test"
  },
  "files": [
    "dist/keysim.js",
    "lib/keysim.js"
  ],
  "keywords": [
    "keyboard",
    "event",
    "dom",
    "KeyboardEvent",
    "simulate"
  ],
  "author": "Brian Donovan",
  "license": "MIT",
  "devDependencies": {
    "6to5": "^2.5.0",
    "esperanto": "^0.5.7",
    "function-bind": "^1.0.2",
    "gobble": "^0.7.1",
    "jsdom": "2.0.0",
    "mocha": "^2.1.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/7..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "iphone/latest",
      "ipad/latest",
      "android-browser/latest"
    ],
    "harness": "mocha-bdd"
  }
}
